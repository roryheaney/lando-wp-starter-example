{
	"$schema": "https://schemas.wp.org/trunk/theme.json",
	"version": 3,
	"settings": {
		"appearanceTools": true,
		"layout": {
			"contentSize": "1024px",
			"wideSize": "1440px"
		},
		"spacing": {
			"blockGap": true,
			"defaultSpacingSizes": false,
			"spacingSizes": [
				{
					"name": "1",
					"slug": "10",
					"size": "clamp(.25rem, .78vmin, .375rem)"
				},
				{
					"name": "2",
					"slug": "20",
					"size": "clamp(.325rem, 1.1vmin, .5rem)"
				},
				{
					"name": "3",
					"slug": "30",
					"size": "clamp(.4375rem, 1.43vmin, .75rem)"
				},
				{
					"name": "4",
					"slug": "40",
					"size": "clamp(.6125rem, 1.93vmin, .9375rem)"
				},
				{
					"name": "5",
					"slug": "50",
					"size": "clamp(.825rem, 2.6vmin, 1.25rem)"
				},
				{
					"name": "6",
					"slug": "60",
					"size": "clamp(1.125rem, 3.5vmin, 1.75rem)"
				},
				{
					"name": "7",
					"slug": "70",
					"size": "clamp(1.5rem, 4.75vmin, 2.3125rem)"
				},
				{
					"name": "8",
					"slug": "80",
					"size": "clamp(2rem, 6.4vmin, 3.125rem)"
				},
				{
					"name": "9",
					"slug": "90",
					"size": "clamp(2.75rem, 8.65vmin, 4.125rem)"
				},
				{
					"name": "10",
					"slug": "100",
					"size": "clamp(3.6875rem, 11.68vmin, 5.625rem)"
				}
			]
		},
		"color": {
			"defaultPalette": false,
			"palette": [
				{
					"name": "Primary",
					"slug": "primary",
					"color": "#066"
				},
				{
					"name": "Secondary",
					"slug": "secondary",
					"color": "indigo"
				},
				{
					"name": "Accent",
					"slug": "accent",
					"color": "aquamarine"
				},
				{
					"name": "Black",
					"slug": "contrast",
					"color": "#000"
				},
				{
					"name": "Dark Gray",
					"slug": "gray-dark",
					"color": "#444"
				},
				{
					"name": "Gray",
					"slug": "gray",
					"color": "#777"
				},
				{
					"name": "Light Gray",
					"slug": "gray-light",
					"color": "#EFEFEF"
				},
				{
					"name": "White",
					"slug": "base",
					"color": "#fff"
				}
			],
			"gradients": [],
			"customDuotone": false
		},
		"custom": {
			"color": {
				"message": "lightblue",
				"success": "honeydew",
				"warning": "lemonchiffon",
				"error": "crimson",
				"required": "var:custom|color|error",
				"link": "navy"
			},
			"spacing": {
				"gap": "var:preset|spacing|70"
			},
			"radius": {
				"default": 0
			},
			"shadow": {
				"color": "0deg 0% 75%",
				"default": "0.4px 0.3px 0.6px hsl(var(--wp--custom--shadow--color) / 0.3), 1.5px 1.3px 2.2px -1.2px hsl(var(--wp--custom--shadow--color) / 0.3), 5.8px 4.8px 8.5px -2.5px hsl(var(--wp--custom--shadow--color) / 0.3)",
				"button-hover-color": "var(--button-hover-shadow-color, hsla(32.38, 86.3%, 57.06%, 0.25))",
				"button-hover": "0 1px 3px hsla(0, 0%, 0%, 0.4), 0 0 0 7px var(--wp--custom--shadow--button-hover-color)"
			}
		},
		"typography": {
			"dropCap": true,
			"fluid": true,
			"defaultFontSizes": false,
			"fontSizes": [
				{
					"name": "Tiny",
					"slug": "x-small",
					"size": "0.875rem"
				},
				{
					"name": "Small",
					"slug": "small",
					"size": "1.0625rem",
					"fluid": {
						"min": "0.875rem",
						"max": "1.0625rem"
					}
				},
				{
					"name": "Baseline",
					"slug": "default",
					"size": "1.25rem"
				},
				{
					"name": "Medium",
					"slug": "medium",
					"size": "1.5rem"
				},
				{
					"name": "Large",
					"slug": "large",
					"size": "1.75rem"
				},
				{
					"name": "Extra-Large",
					"slug": "x-large",
					"size": "2.5rem"
				},
				{
					"name": "XXL",
					"slug": "xx-large",
					"size": "3.25rem"
				},
				{
					"name": "XXXL",
					"slug": "xxx-large",
					"size": "4rem"
				}
			],
			"fontFamilies": [
				{
					"name": "System Fonts",
					"slug": "system",
					"fontFamily": "system-ui, sans-serif"
				},
				{
					"name": "Display",
					"slug": "display",
					"fontFamily": "Avenir, Montserrat, Corbel, 'URW Gothic', source-sans-pro, sans-serif"
				},
				{
					"name": "Monospace",
					"slug": "monospace",
					"fontFamily": "ui-monospace, 'Cascadia Code', 'Source Code Pro', Menlo, Consolas, 'DejaVu Sans Mono', monospace;"
				}
			]
		}
	},
	"styles": {
		"typography": {
			"fontFamily": "var:preset|font-family|system",
			"fontSize": "var:preset|font-size|default",
			"lineHeight": "1.5"
		},
		"color": {
			"text": "var:preset|color|contrast",
			"background": "var:preset|color|base"
		},
		"spacing": {
			"blockGap": "var:custom|spacing|gap"
		},
		"elements": {
			"heading": {
				"typography": {
					"fontFamily": "var:preset|font-family|display"
				}
			},
			"h1": {
				"typography": {
					"fontSize": "var:preset|font-size|xx-large",
					"lineHeight": "1.2"
				}
			},
			"h2": {
				"typography": {
					"fontSize": "var:preset|font-size|x-large",
					"lineHeight": "1.3"
				}
			},
			"h3": {
				"typography": {
					"fontSize": "var:preset|font-size|large",
					"lineHeight": "1.3"
				}
			},
			"h4": {
				"typography": {
					"fontSize": "var:preset|font-size|default"
				}
			},
			"link": {
				"color": {
					"text": "var:custom|color|link"
				},
				":hover": {
					"typography": {
						"textDecoration": "none"
					}
				},
				":active": {
					"typography": {
						"textDecoration": "none"
					}
				}
			},
			"button": {
				"border": {
					"radius": "var:custom|radius|default",
					"color": "transparent",
					"width": "2px",
					"style": "solid"
				},
				"spacing": {
					"padding": {
						"top": "var:preset|spacing|30",
						"right": "var:preset|spacing|30",
						"bottom": "var:preset|spacing|30",
						"left": "var:preset|spacing|30"
					}
				},
				"typography": {
					"textDecoration": "none !important"
				},
				"color": {
					"background": "var:preset|color|primary",
					"text": "var:preset|color|base"
				},
				":hover": {
					"color": {
						"background": "var:preset|color|secondary",
						"text": "var:preset|color|base"
					}
				},
				":focus": {
					"color": {
						"background": "var:preset|color|secondary",
						"text": "var:preset|color|base"
					}
				},
				":visited": {
					"color": {
						"background": "var:preset|color|secondary",
						"text": "var:preset|color|base"
					}
				}
			},
			"caption": {
				"color": {
					"text": "var:preset|color|gray-dark"
				},
				"typography": {
					"fontSize": "var:preset|font-size|small"
				}
			},
			"cite": {
				"color": {
					"text": "var:preset|color|gray-dark"
				},
				"typography": {
					"fontSize": "var:preset|font-size|small"
				}
			}
		},
		"blocks": {}
	},
	"templateParts": []
}