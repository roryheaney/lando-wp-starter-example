{
	"private": true,
	"scripts": {
		"dev": "npm run development && npm run devsass",
		"development": "cross-env webpack --mode development --progress --config webpack/webpack.config.js",
		"prod": "npm run production",
		"production": "cross-env webpack --mode production --progress --config webpack/webpack.config.js",
		"watch": "npm run development -- --watch && webpack-dev-server --progress --colors",
		"devsass": "sass assets/styles/common.scss:public/styles/common.css",
		"sasswatch": "sass --watch --trace --update assets/styles/common.scss:public/styles/common.css",
		"prodsass": "sass assets/styles/common.scss:public/styles/common.css --style=compressed"
	},
